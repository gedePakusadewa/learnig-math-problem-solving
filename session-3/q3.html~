	</body>
	<script>
		function isPrimeNumber(num = 0){
			for(var i = 2; i < num; i ++ ){
				if((num % i) === 0){
					return false; 
				}
			}

			return true;
		}
		
		function loopPrimeNumberRange(range = 0){
			var i = 0, limit = range;
			for(i = 2; i <= limit; i++){
				isPrimeNumber(i);
			}
		}
			
		//loopPrimeNumberRange(100);
		
		function tes(digit){
			var i = 2, num = digit, increNum = 0;
			var listNum = [];
			for(i < digit; i++){
				if((num % i) === 0){
					listNum[increNum] = i;
					num = (num / i);
					i = 2;
					increNum++;
				}
			}
			return listNum;
		}		

		console.log(tes(144));
		//var tes12 = 7213;
		//console.log("Is " + tes12 + " Prime Number -> " + isPrimeNumber(tes12));
		//console.log(21 % 2);
		
	</script>

</html>
