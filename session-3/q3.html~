	<script>
		function calculatePrimeNumber(num = 0){
			var increNum = 0;
			var basicNumber = [2, 3, 4, 5, 6, 7, 8, 9];
			/*
			for(var i = 0; i < basicNumber.length; i++){
				console.log(basicNumber[i]);
			}
			*/

			var result = 0;
			var isPrimeNum = true;
			var num1 = num;

			while(increNum < basicNumber.length){
				result = num1 % basicNumber[increNum];
				//console.log(basicNumber[increNum]);
				if(result === 0){
					isPrimeNum = false;
					//console.log("---> " + basicNumber[increNum]);
				//	break;
				}
				increNum++;
			}

			if(isPrimeNum === true){
				console.log(num1 + "---> " + isPrimeNum);
			}
		}

		function loopPrimeNumberRange(range = 0){
			var i = 0, limit = range;
			for(i = 2; i <= limit; i++){
				calculatePrimeNumber(i);
			}
		}
			
