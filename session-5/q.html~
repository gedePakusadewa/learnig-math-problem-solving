	</body>
	<script>
		var sumLoct = 4;
		var listLoct = ["1,5", "900000000,78", "392,123", "623,134"];
		var digit1 = 0, digit2 = 0;
		var sumNum = 0;
		for(var i = 0; i < listLoct.length; i++){
			//console.log(listLoct[i]);
			
			if(!isValid(sumLoct, listLoct)){
				break;
			}

			if((i + 1) === listLoct.length){
				break;	
			}else{
				sumNum = sumNum + splitArray(listLoct[i], listLoct[i + 1]);
			}
		
		}

		function splitArray(data1 = "", data2 = ""){
			var tmp = data1.split(",");
			var tmp2 = data2.split(",");
			return Math.abs(tmp[0] - tmp2[0]) + Math.abs(tmp[1] - tmp2[1]); 
		}
		
		function isValid(totalLoct = 0, arrayData){
			var condition = true;

			if(totalLoct >= 200000 || totalLoct === 0){
				condition = false;
			}
			
			if(!isValidArrayData(listLoct)){
				condition = false;
			}

			return condition;
		}
		
		function isValidArrayData(dataIn){
			var data = dataIn
			var i = 0;
			var condition = true;
			var tmp;
			var limit = 1000000000;
			for(i = 0; i < data.length; i++){
				tmp = data[i].split(",");
				if((tmp[0] >= limit) || (tmp[1] >= limit)){
					condition = false;
					break;
				}
			}
			return condition;
		}

		function modulo(num){
			return num % 1000000007;
		}
		
		sumNum = modulo(sumNum);

		//var tes = ["1,2", "1234567891, 18475", "73649, 19341", "1, 862"];
		console.log(sumNum);
	</script>
