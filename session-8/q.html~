<!DOCTYPE html>
<html>
	<head>
		<meta charset = "utf-8" />
	</head>
	<body></body>
	<script>
		//question from kecek in folder GDP question 2

		function test(){
			var data = [17, 49, 50, 10, 33, 21];
			var price = 100, cut = 10, totalCut = 0, totalLeftOver = 0, total = 0, fixTotal = 0;
			var low = lowest(data);
			if(data.length <= 1000 && data.length >= 1){
				if(low >= 1 && low <= 10000 ){
					if((price >= 1 && price <= 100000000) && (cut >= 1 && cut <= 100000000)){	
						for(var i = 0; i < data.length; i++){	
							totalCut = totalCut + sumMod(data[i], low);
							totalLeftOver = totalLeftOver + (data[i] - (sumMod(data[i], low) * low));
							leftOver(data[i], low);
						}
						
						total = (totalCut * price) - (totalCut * cut - 1);	
						fixTotal = total + (totalLeftOver * price) - (totalLeftOver * cut);
					}
				}
			}
			//console.log(totalCut);
		}

		function leftOver(digit = 0, lowest = 0){
			var tmp = digit, incre = 0;
			while(tmp >= lowest){
				tmp = tmp - lowest;
			}
			console.log(tmp);
			return incre;
		}

		function lowest(data){
			var tmp = data;
			var k = 0, i = 0, value = tmp[0];
			
			for(k = 0; k < tmp.length, k++){
				if(tmp[k] > 0){
					value = tmp[k];
				}
			}
			
			while(i < tmp.length){
				if((i + 1) < tmp.length){
					//console.log(tmp[i] + " < " + tmp[i + 1] + " -> value = " + value);
					if(tmp[i] <= tmp[i + 1]){
						if(tmp[i] < value){	
							value = tmp[i];
						}	
					}else{
						if(tmp[i + 1] < value){
							value = tmp[i + 1];
						}
					}
				}
				i++;		
			}
			return value;
		}

		function sumMod(digit = 0, lowest = 0){
			var tmp = digit, incre = 0;
			while(tmp >= lowest){
				tmp = tmp - lowest;
				incre++;
			}
			return incre;
		}

		test();
	//	(sumMod(27, 3));

	</script>
</html>
