	<body></body>
	<script>
		//question from kecek in fodler GDP question 2
		function calculate(cost = 0, price = 0, sum = 0, listSteel){
			var c = cost, p = price, n = sum;
			var list = listSteel;

			
		}	

		function test(){
			var data = [45, 817, 12, 40, 87, 6, 19, 34];
			var price = 100, cut = 20, totalCut = 0;
			var lowest = lowest(data);

			for(var i = 0; i < data.length; i++){
				totalCut = totalCut + sumMod(data[i]);
			}

			var total = (totalCut * price) - (totalCut * cut);	
			return total;
		}

		function lowest(data){
			var tmp = data;
			var i = 0, value = tmp[0];
			
			while(i < tmp.length){
				if((i + 1) < tmp.length){
					//console.log(tmp[i] + " < " + tmp[i + 1] + " -> value = " + value);
					if(tmp[i] <= tmp[i + 1]){
						if(tmp[i] < value){	
							value = tmp[i];
						}	
					}else{
						if(tmp[i + 1] < value){
							value = tmp[i + 1];
						}
					}
				}
				i++;		
			}
			return value;
		}

		function sumMod(digit = 0, lowest = 0){
			var tmp = digit, incre = 0;
			while(tmp >= lowest){
				tmp = tmp - lowest;
				incre++;
			}
			return incre;
		}

		//test();
		(sumMod(27, 3));

	</script>
