	function calculateN(num, rangeNum){
		var numIncre = 0, numStatic = num, range = rangeNum, finalResult = 0, sumNumberFinal = 0;
		var statusStop = false;

		while(statusStop === false){
			if((numIncre + numStatic) > range){
				statusStop = true;
			}else{
				numIncre += numStatic;
				finalResult++;
				sumNumberFinal += numIncre;
				console.log(numIncre);
			}
	
		}
		//return sumNumberFinal;
	}

	

	console.log(calculateN(5, 20));
