	function findMultiNum(num = 0, range = 0){
		var coreNum = num, rangeNum = range, result = 0, increNumber = 0;
		var stopNow = false;

		while(stopNow === false){
			if(((increNumber + 1) * coreNum) < rangeNum){
				increNumber++;
				result = coreNum * increNumber;
			}else{
				stopNow = true;
			}
		}
		
		//console.log("Number -> " + increNumber);
		return increNumber;
	}

	function main(num = 0, range = 0){
		return sumNumMulti(num, findMultiNum(num, range));
	}

	function sumNumMulti(num = 0, multi = 0){
		var coreNum = num, multiNum = multi, increNum = 0, result = 0;
		var stopNow = false;

		while(stopNow === false){
			result = result + (coreNum * increNum);
			if(increNum === multiNum){
				stopNow = true;	
			}else{
				increNum++;
			}
		}
		return result;
		//console.log("Sum Number -> " + result);
	}
	
	function sumSameNumber(num1 = 0, num2 = 0, range = 0){
		var numTotal = 0, coreNum1 = num1, coreNum2 = num2, limit = range, increNum= 0;
		var multiCoreNum1 = findMultiNum(coreNum1, limit), multiCoreNum2 = findMultiNum(coreNum2, limit), result1 = 0, result2 = 0;

		if(multiCoreNum1 < multiCoreNum2 ){
			limit = multiCoreNum2;
		}else{
			limit = multiCoreNum1;
		}

		while(increNum !== limit){
			
			result1 = coreNum1 * increNum;
			resutl2 = coreNum2 * increNum;
			
			if((result1 === result2 && (result1 !== 0)){
				numTotal += increNum;
			}

			increNum++;
			
		}

		console.log(numTotal);
	}
		
	sumSameNumber(3, 5, 20);
	
	//console.log(main(3, 1000));
