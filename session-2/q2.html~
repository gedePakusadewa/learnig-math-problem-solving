	<script>
		function fibonacci(range = 0){
			var num1 = 1, num2 = 2, num3 = 0, limit = range, totalNum = 0;

			while((num3 + num1) < limit){
				num3 = num1 + num2;
				//console.log(num3 + " = " + num1 + " + " + num2);
				num1 = num2;
				num2 = num3;
				if((num3 % 2) === 0){
					totalNum += num3;
					console.log(num3);
				}
			}
		}

		fibonacci(620);
		//console.log(647252%2);
	</script>
